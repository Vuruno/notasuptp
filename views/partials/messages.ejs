<% if(locals.errors){ %>
    <% errors.forEach(function(error) { %>
        <div class="alert alert-dismissible alert-danger" role="alert">
            <%= error %>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                    <!-- <span aria-hidden="true">&times;</span> -->
                </button>
        </div>
        <% }); %>
            <% } %>